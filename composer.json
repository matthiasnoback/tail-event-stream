{
  "name": "matthiasnoback/tail-event-stream",
  "description": "A simple, tail-based event stream",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "beberlei/assert": "^3.3",
    "symfony/process": "^5.3 || ^6.0",
    "ext-pcntl": "*"
  },
  "autoload": {
    "psr-4": {
      "TailEventStream\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TailEventStream\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4",
    "matthiasnoback/phpunit-asynchronicity": "^2.4",
    "phpunit/phpunit": "^9.5",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-beberlei-assert": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
